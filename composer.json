{
    "name": "directorytree/anonymize",
    "description": "A Laravel package for data anonymization",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "orchestra/testbench": "^9.0|^10.0",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "DirectoryTree\\Anonymize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\Anonymize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DirectoryTree\\Anonymize\\AnonymizeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
